{
  "regionName": "eu-west-1",
  "httpPort": 8081,
  "timeoutPercentageThreshold": 0,
  "failurePercentageThreshold": 0,
  "turbine": {
    "host": "${TURBINE_HOST}",
    "port": ${TURBINE_PORT},
    "path": "/turbine/turbine.stream?cluster=",
    "secure": true,
    "auth": {
      "username": "username",
      "password": "password"
    }
  },
  "entryClusters": [
    "prod-proxy",
    "someinternalcomponent"
  ],
  "internetClusters": [
    "prod-proxy"
  ],
  "hystrixGroupToCluster": [
    {
      "group": "hystrix-prod-proxy",
      "cluster": "prod-proxy"
    }
  ]
}
